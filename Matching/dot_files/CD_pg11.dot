digraph "Causal Diagramm RDD" {
  layout=neato
  fontname="Helvetica,Arial,sans-serif"
  bgcolor=transparent
  imagescale=both
  node [fontname="Helvetica,Arial,sans-serif"]
  edge [fontname="Helvetica,Arial,sans-serif"]
  node [shape=none];
  "A" [pos="0,2!"]
  "B" [pos="2,2!"]
  "C" [pos="4,2!"]
  "Treatment" [pos="0,0!"]
  "Outcome" [pos="4,0!"]


  
  "A" -> "Treatment"
   "B" ->  "Treatment"
  "B" -> "Outcome"
  "A" -> "Outcome"
  "C" -> "Treatment"
  "C" -> "Outcome"
   "Treatment"  -> "Outcome"
}